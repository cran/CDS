\name{testDate}
\alias{testDate}
\title{function for testing the date entered}
\usage{
testDate(date)
}
\arguments{
  \item{date}{date entered}
}
\value{
TRUE if date is valid, FALSE if date is in future, and
"Invalid date format. Must be YYYY-MM-DD" if format is
wrong
}
\description{
function for testing the date entered
}

